{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri_demo",
  "version": "0.1.0",
  "identifier": "com.atuya.tauri_demo",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "BlurWindow Demo",
        "width": 450,
        "height": 800,
        "resizable": true,
        "minWidth": 400,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": [
      "libs/blurwindow.dll"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}